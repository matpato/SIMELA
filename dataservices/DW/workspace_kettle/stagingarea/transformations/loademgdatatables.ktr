        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.*;
import java.io.*;
import java.lang.Object;


private ArrayList x_array = null;
private ArrayList y_array = null;
private double[] x_normalized = null;
private double[] y_envelope = null;
private int[] idx = null;
private ArrayList tmp = null;

private double pat_muscle = 0;
private Long emg_id = null;
private double x = 0;
private double y = 0;
private int i,j = 0;
private double min_x = 0;
private double dif_max_min_x = 0;
private double average_y = 0;
private double area = 0;
private Long nr_peaks = 0l;



public double calculateAverage(ArrayList al) {
  double sum = 0;
  if(!al.isEmpty()) {
    for (i = 0; i < al.size(); i++){
        sum += (Double)al.get(i);
    }
    return sum / al.size();
  }
  return sum;
}


public long calculateNrPeaks(double[] y_envelope, double pat_muscle, double threshold_noise, int window) {
  int nr_peaks = 0;
  boolean peak_found = false;
  i=0;	 
  while (i < (y_envelope.length - window)) {		
		if ((y_envelope[i] - pat_muscle) > threshold_noise) {
			if(!peak_found) {
				nr_peaks += 1;
				peak_found = true;
			}
		}
		else {
			if(peak_found) {
				for (j = (i + 1); j <=( i + window) ; j++) {
					if ((y_envelope[j] - pat_muscle) > threshold_noise) {
						break;
					}
				}
				if (j > (i + window))
					peak_found = false;

				i = j + 1;
			}	
		}
		i++;
  }
  //last window - maybe one more peak
  for (j = i; j < y_envelope.length ; j++) {		
		if ((y_envelope[i] - pat_muscle) > threshold_noise) {
			if(!peak_found) {
				nr_peaks += 1;
				break;
			}
		}
  }

  return (long)nr_peaks;
}



public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();

	if (r == null) {

		if(!y_array.isEmpty()) {
			//envelope
			y_envelope = new double[y_array.size()];
			average_y = calculateAverage(y_array);

			for (i = 0; i < y_array.size(); i++)
				y_envelope[i] = Math.abs((Double)y_array.get(i) - average_y);

			//calculate number of peaks	
			nr_peaks = calculateNrPeaks(y_envelope, pat_muscle, Double.valueOf(getVariable("threshold_noise")), Integer.valueOf(getVariable("window")));

			//calculate area AUC if nr_peaks sup 0
			if (nr_peaks > 0) {
				//sort into idx and normalization 
				tmp = new ArrayList(x_array);
				Collections.sort(x_array);

				idx = new int[x_array.size()];
				x_normalized = new double[x_array.size()];
			
				min_x = (Double)x_array.get(0);
				dif_max_min_x = (Double)x_array.get(x_array.size()-1) - min_x;


				for (i = 0; i < x_array.size(); i++){
    				idx[i] = tmp.indexOf(x_array.get(i));
					x_normalized[i] = ((Double)x_array.get(i)- min_x)/dif_max_min_x;
				}

				//calculate area AUC
				area = 0;
				for (i = 0; i < (x_array.size() - 1); i++)
					area += (x_normalized[i+1] - x_normalized[i])*(y_envelope[idx[i+1]] + y_envelope[idx[i]])/2;
			}
  
		}

		// create a row
		Object[] res = RowDataUtil.allocateRowData(data.outputRowMeta.size());

    	// Set a value in a new output field
    	get(Fields.Out, "emg").setValue(res, emg_id);
    	get(Fields.Out, "area").setValue(res, area);
    	get(Fields.Out, "nr_peaks").setValue(res, nr_peaks);

		// Send the row on to the next step.
   		 putRow(data.outputRowMeta, res);

		setOutputDone();
		return false;
	}

    if (first){

		emg_id = get(Fields.In, "emg_id").getInteger(r);
    	pat_muscle = get(Fields.In, "pat_muscle").getNumber(r); 
		x_array = new ArrayList();
		y_array = new ArrayList();
        first = false;
    }
   

    x = get(Fields.In, "x").getNumber(r);
	x_array.add(x);
    y = get(Fields.In, "y").getNumber(r);
	y_array.add(y);

   


	return true;
}


]]></class_source>
